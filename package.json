{
    "name": "react-simple-card",
    "version": "2.0.3",
    "description": "Simple React card component",
    "author": "ummahusla",
    "license": "MIT",
    "repository": "ummahusla/react-simple-card",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "jsnext:main": "dist/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "keywords": [
        "react",
        "card"
    ],
    "scripts": {
        "test": "react-scripts-ts test --env=jsdom",
        "coverage": "CI=true react-scripts-ts test --env=jsdom --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "build": "rollup -c",
        "start": "rollup -c -w",
        "prepare": "yarn run build",
        "predeploy": "cd example && yarn install && yarn run build",
        "deploy": "npm publish"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{tsx}",
            "!/node_modules/",
            "!src/index.tsx"
        ]
    },
    "peerDependencies": {
        "prop-types": "^15.5.4",
        "react": "^15.0.0 || ^16.0.0",
        "react-dom": "^15.0.0 || ^16.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@svgr/rollup": "^2.4.1",
        "@types/classnames": "^2.2.9",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^23.1.5",
        "@types/react": "^16.8.6",
        "@types/react-dom": "^16.8.6",
        "babel-core": "^6.26.3",
        "babel-jest": "^24.9.0",
        "babel-runtime": "^6.26.0",
        "classnames": "^2.2.6",
        "coveralls": "^2.13.1",
        "cross-env": "^5.1.4",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "enzyme-to-json": "^3.4.3",
        "node-sass": "^7.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts-ts": "^4.0.8",
        "rollup": "^0.62.0",
        "rollup-plugin-babel": "^3.0.7",
        "rollup-plugin-commonjs": "^9.1.3",
        "rollup-plugin-node-resolve": "^3.3.0",
        "rollup-plugin-peer-deps-external": "^2.2.0",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-typescript2": "^0.24.3",
        "rollup-plugin-url": "^1.4.0",
        "typescript": "^2.8.3"
    },
    "files": [
        "dist"
    ]
}
